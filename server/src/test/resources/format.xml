<settings>

    <localRepository>/Applications/MaxJDE/repository</localRepository>

    <servers>
        <!-- Distribution repository server -->
        <server>
            <username>admin</username>
            <password>password</password>
            <id>utvzon01</id>
        </server>
    </servers>

    <mirrors>
    </mirrors>

    <profiles>
        <!-- | MaxM repository overwrites central repository. This prevents 
            users from downloading | jar files that are not in current development release. -->
        <profile>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <id>MaxM</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <name>MaxM repository</name>
                    <url>http://utvzon01.maxm.se:8081/artifactory/repo</url>
                    <layout>default</layout>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>central</id>
                    <name>MaxM repository</name>
                    <url>http://utvzon01.maxm.se:8081/artifactory/repo</url>
                    <layout>default</layout>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>MaxM</activeProfile>
    </activeProfiles>

</settings>
